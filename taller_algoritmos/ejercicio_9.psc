Algoritmo Inicio_Sueldo_comision
	Escribir "ingrese su sueldo base"
	Leer sb
	Escribir "ingrese el valor de la venta #1 "
	Leer v1
	Escribir "ingrese el valor de la venta #2  "
	Leer v2
	Escribir "ingrese el valor de la venta #3 "
	Leer v3
	com<-(v1+v2+v3)*0.10
	Escribir "la comision es de  " com
	st<-com+sb
	Escribir "el suedo total seria   " st

FinAlgoritmo
